<views:BaseView x:Class="RonaApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RonaApp"
        xmlns:views="clr-namespace:RonaApp.Views.Base"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <!--Ejer1 - Introducir los comandos para que los botones tengan funcionalidad-->
        <Button  Width="100" Height="50" Command="{Binding ShowRegisterViewModelCommand}">Register patient</Button>
        <Button Margin="0,16,0,0" Width="100" Height="50" Command="{Binding ShowPatientsViewModelCommand}">Show patients</Button>
    </StackPanel>
</views:BaseView>
